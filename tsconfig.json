{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "module": "ESNext",
        "skipLibCheck": true,
        "moduleResolution": "NodeNext",
        "isolatedModules": true,
        "noEmit": false,
        "emitDeclarationOnly": false,
        "outDir": "dist",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true
    },
    "include": [
        "src"
    ]
}
